<beans
	default-destroy-method="destroy"
	default-init-method="afterPropertiesSet"
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
>
	<bean id="com.liferay.portal.kernel.sql.SQLTransformerUtil" class="com.liferay.portal.kernel.sql.SQLTransformerUtil">
		<constructor-arg>
			<bean class="com.liferay.portal.dao.orm.common.SQLTransformer" />
		</constructor-arg>
	</bean>
	<bean id="com.liferay.portal.service.persistence.ReleasePersistence" class="com.liferay.portal.service.persistence.impl.ReleasePersistenceImpl" parent="basePersistence" />
	<bean id="com.liferay.portal.service.ReleaseLocalService" class="com.liferay.portal.service.impl.ReleaseLocalServiceImpl" />
</beans>
